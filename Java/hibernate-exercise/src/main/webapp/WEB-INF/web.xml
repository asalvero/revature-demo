<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>AddEmployeeServlet</servlet-name>
    <servlet-class>org.example.AddEmployeeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddEmployeeServlet</servlet-name>
    <url-pattern>/AddEmployeeServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ListEmployeeServlet</servlet-name>
    <servlet-class>org.example.ListEmployeeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ListEmployeeServlet</servlet-name>
    <url-pattern>/ListEmployeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
